{
  "expo": {
    "name": "school-mobile-app-ios",
    "slug": "school-mobile-app-ios",
    "platforms": ["ios", "android"],
    "privacy": "public",
    "version": "1.2.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/Images/salad.png",
      "sounds": [
        {
          "name": "default",
          "filename": "./assets/Images/maybe-one-day-584.mp3"
        },
        {
          "name": "custom",
          "filename": "./assets/Images/maybe-one-day-584.mp3"
        }
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Images/salad.png"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos"
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ]
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["tsx", "js", "jsx", "scss", "sass"],
      "transformer": "node_modules/react-native-sass-transformer/index.js"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      },
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true,
        "com.apple.developer.applesignin": ["Default"]
      },
      "bundleIdentifier": "Second-dayofSwift-Testing"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "blockedPermissions": ["android.permission.RECORD_AUDIO"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.goglobal_school.schoolmobileappios"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4aaf66e1-852f-4c99-806f-08101561f3c3"
      }
    }
  }
}
